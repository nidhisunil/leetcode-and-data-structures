def maxProfit(self, prices: List[int]) -> int:
        l, r = 0, 1
        maximum_profit = 0
        while (r<len(prices)):
            if prices[l]<prices[r]:
                maximum_profit = max(maximum_profit, prices[r]-prices[l])
            else:
                l = r
            r = r+1

        return maximum_profit
